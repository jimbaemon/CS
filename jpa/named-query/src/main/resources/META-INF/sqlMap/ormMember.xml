<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" version="2.1">

    <named-query name="Member.findByUserNameXml">
        <query>
            <![CDATA[
                SELECT
                    m
                FROM Member m
                WHERE m.username = :username
            ]]>
        </query>
    </named-query>

</entity-mappings>